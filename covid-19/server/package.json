{
  "name": "covid-19",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start1": "nodemon server.ts",
    "start": "ts-node-dev --ignore-watch './db' --project 'tsconfig.json' server.ts",
    "clean": "rimraf ./build",
    "tsc": "tsc --project 'tsconfig.json'",
    "copy": "cp -R ./db ./build",
    "prod": "node ./build/server.js",
    "build": "npm run clean && npm run tsc && npm run copy"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "deep-diff": "^1.0.2",
    "express": "^4.17.1",
    "humps": "^2.0.1",
    "i18n-iso-countries": "^6.0.0",
    "jsonfile": "^6.0.1",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "rxjs": "^6.6.3",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jsonfile": "^6.0.0",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.14.7",
    "lint-staged": "^10.3.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~4.0.2"
  }
}
