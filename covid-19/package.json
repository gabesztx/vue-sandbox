{
  "name": "covid-19-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "serv:dev": "ts-node-dev --ignore-watch './db' --project 'tsconfig.json' server.ts",
    "serv:compiler": "tsc --project 'tsconfig.json'",
    "serv:start": "node ./build/server.js",
    "serv:build": "npm run clean && npm run tsc && npm run copy",
    "i18n:report": "vue-cli-service i18n:report --src 'src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "clean": "rimraf ./build",
    "copy": "cp -R ./db ./build"
  },
  "dependencies": {
    "@vue/composition-api": "^1.0.0-beta.11",
    "axios": "^0.20.0",
    "buefy": "^0.9.3",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "vee-validate": "^3.3.9",
    "vue": "^2.6.12",
    "vue-i18n": "^8.17.3",
    "vue-router": "^3.4.3",
    "vuex": "^3.5.1",
    "body-parser": "^1.19.0",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "deep-diff": "^1.0.2",
    "humps": "^2.0.1",
    "i18n-iso-countries": "^6.0.0",
    "jsonfile": "^6.0.1",
    "rxjs": "^6.6.3",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-plugin-router": "^4.5.8",
    "@vue/cli-plugin-typescript": "^4.5.8",
    "@vue/cli-plugin-vuex": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.1.0",
    "eslint": "^7.8.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "typescript": "~4.0.2",
    "vue-cli-plugin-buefy": "^0.3.7",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.12",
    "@types/body-parser": "^1.19.0",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jsonfile": "^6.0.0",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.14.7",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0"
  }
}
