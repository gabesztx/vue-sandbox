{
  "name": "covid-19-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "serv:dev": "ts-node-dev --ignore-watch './server/db' --project './server/tsconfig.json' server/server.ts",
    "serv:compiler": "tsc --project './server/tsconfig.json'",
    "serv:start": "node ./dist/server.js",
    "build:all": "npm run build && npm run serv:compiler && npm run copy",
    "clean": "rimraf ./dist",
    "copy": "cp -R ./server/db ./dist/",
    "i18n:report": "vue-cli-service i18n:report --src 'src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "@vue/composition-api": "^1.0.0-beta.19",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "buefy": "^0.9.4",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.7.0",
    "cors": "^2.8.5",
    "deep-diff": "^1.0.2",
    "express": "^4.17.1",
    "humps": "^2.0.1",
    "i18n-iso-countries": "^6.2.2",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "rxjs": "^6.6.3",
    "unirest": "^0.6.0",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.12",
    "vue-i18n": "^8.22.2",
    "vue-router": "^3.4.9",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-plugin-router": "^4.5.9",
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-plugin-vuex": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "typescript": "~4.0.2",
    "vue-cli-plugin-buefy": "^0.3.7",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.12",
    "@types/body-parser": "^1.19.0",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jsonfile": "^6.0.0",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.9",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0"
  }
